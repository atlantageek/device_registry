table{ border-collapse: collapse; width: 400px; color: black; margin: auto;}
table, tr, th, td{ border: 1px solid black; } 
th{cursor: pointer;  background-repeat:no-repeat; background-position: 3% center;  }
table#report_table {border: 1px solid black;}
table#report_table tr th.asc{background-image:url(../img/asc.gif);}
table#report_table tr th.des{background-image:url(../img/des.gif);}
tr{background-color:#CDE1E4;}
tr.alt{background-color:#94BEC5;}
